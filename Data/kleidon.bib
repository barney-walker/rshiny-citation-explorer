@article{ ISI:000306264200011,
Author = {Ross, John and Corlan, Alexandru D. and Mueller, Stefan C.},
Title = {{Proposed Principles of Maximum Local Entropy Production}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY B}},
