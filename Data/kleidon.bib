@article{ ISI:000311296500001,
Author = {Frank, S. A.},
Title = {{Natural selection. V. How to read the fundamental equations of
   evolutionary change in terms of information theory}},
Journal = {{JOURNAL OF EVOLUTIONARY BIOLOGY}},
